function {
  if [[ ! -e "/etc/ssh/ssh_host_ed25519_key" ]]; then
    ssh-keygen -t ed25519 -f /etc/ssh/ssh_host_ed25519_key < /dev/null
    status $? "generated ed25519 host key"
  else
    success "skipped /etc/ssh/ssh_host_ed25519_key"
  fi

  if [[ ! -e "/etc/ssh/ssh_host_rsa_key" ]]; then
    ssh-keygen -t rsa -b 4096 -f /etc/ssh/ssh_host_rsa_key < /dev/null
    status $? "generated rsa host key"
  else
    success "skipped /etc/ssh/ssh_host_rsa_key"
  fi
}
